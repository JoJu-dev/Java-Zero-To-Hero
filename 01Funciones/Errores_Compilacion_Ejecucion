String err1(){
    return "Dondé está el error?";
}
err1()
String err2(){
    return "String o string?";
}
err2()
String err3(String who){
    return String.format("%s, cuál es el error?",who);
}
err3("err3")
String err4(){
    return "Este es un error sutil";
}
err4()
String err5(){
    return "Este es un poco menos sutil";
}
err5()
String err6(){
    return "Hola Error!";
}
err6()
String err7(String quien){
    return String.format("%s, Un poco mañoso, no?", quien);
}
err7("Error")