diferenciaDeCuadrados(3,4)
int diferenciaDeCuadrados(int a, int b) {
    var cuadradoDeA = a * a;
    var cuadradoDeB = b * b;
    var diferenciaDeCuadrados = cuadradoDeA - cuadradoDeB;
    return diferenciaDeCuadrados;
}
diferenciaDeCuadrados(3,4)
diferenciaDeCuadrados(2,2)
distancia(0.0,0.0,1.0,1.0)
distancia(0.0,0.0,1.0,1.0)
double distancia(double x1, double y1, double x2, double y2) {
    var coordenadasPrimerPunto = Math.pow((x2 - x1),2);
    var coordenadasSegundoPunto = Math.pow((y2 - y1),2);
    var d = Math.sqrt((coordenadasPrimerPunto + coordenadasSegundoPunto));
    return d;
}
