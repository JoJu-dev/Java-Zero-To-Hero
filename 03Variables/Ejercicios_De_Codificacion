double fahrenheitACelsius(double F) { 
    var C = (( F - 32.0 ) * ( 5.0 / 9.0 ));
    return C;
}
fahrenheitACelsius(60.0)
fahrenheitACelsius(32.0)
double gravedadSuperficial(double masa, double radio) {
    var G = 6.67430e-11;
    var gravedadSuperficial = ( G * masa ) / Math.pow(radio,2.0);
    return gravedadSuperficial;
}
gravedadSuperficial(5.972e24,6.378e6)
gravedadSuperficial(6.421e23,3.3972e6)
double gravedadSuperficialDeLaTierra() {
    var G = 6.67430e-11;
    var MASA_DE_LA_TIERRA = 5.972e24; 
    var RADIO = 6.378e6;
    var gravedadSuperficial = ( G * MASA_DE_LA_TIERRA ) / Math.pow(RADIO,2.0);
    return gravedadSuperficial;
}
gravedadSuperficialDeLaTierra()
pesoEnMarte(80.0)
pesoEnMarte(80.0)
double pesoEnMarte(double pesoTierra) {
    var MASA_EN_MARTE = 6.421e23;
    var RADIO_EN_MARTE = 3.3972e6;
    var gravedadSuperficialEnMarte = gravedadSuperficial( MASA_EN_MARTE, RADIO_EN_MARTE );
    var pesoEnMarte = ( ( gravedadSuperficialEnMarte / gravedadSuperficialDeLaTierra() ) * pesoTierra );
    return pesoEnMarte;
}
