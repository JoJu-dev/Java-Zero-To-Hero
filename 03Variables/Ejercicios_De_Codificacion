double LeyCosenos(double a, double b, double gamma) {
var AnguloEntreAyB = Math.toRadians(gamma);
var terminoDelCoseno = 2 * a * b * AnguloEntreAyB;
var sumaDeCuadrados = (Math.pow(a,2) + Math.pow(b,2));
var c = Math.sqrt(sumaDeCuadrados - terminoDelCoseno);
return c;
}
LeyCosenos(3.0,4.0,90.0)
LeyCosenos(3.0,4.0,90.0)
leyCosenos(3.0, 4.0, 90.0)
leyCosenos(5,7,60)
double leyCosenos(double a, double b, double gamma) {
var AnguloEntreAyBRadianes = Math.toRadians(gamma);
var AnguloEntreAyBCoseno = Math.cos(AnguloEntreAyBRadianes);    
var terminoDelCoseno = 2 * a * b * AnguloEntreAyBCoseno;
var sumaDeCuadrados = (Math.pow(a,2) + Math.pow(b,2));
var c = Math.sqrt(sumaDeCuadrados - terminoDelCoseno);
return c;
}
leyCosenos(3.0, 4.0, 90.0)
leyCosenos(11.0, 11.0, 60.0)